<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <!-- Paste the PersonalAccessToken inside the quotes next to 'value=' below -->
    <add key="PersonalAccessToken" value="" />
    <add key="TeamSite" value=""/>
    <add key="TeamProject" value=""/>
    <add key="WordTemplate" value="FuncSpec.docx" />
    <add key="HeadersColor" value="#008AC8" />
    <add key="ColorReplacements" value="rgb(0, 0, 0);black;#f0f0f0;windowtext" />
    <!--<add key="WorkItemsQuery" value="SELECT [System.Id], [System.Title], [System.Description] FROM WorkItems WHERE [System.WorkItemType] IN ('User Story') AND [System.TeamProject] = '{0}' ORDER BY [System.Id]" />-->
    <add key="WorkItemsQuery" value="Select [System.Id], [System.WorkItemType], [System.Title], [System.Description], [System.State] From WorkItemLinks WHERE (Source.[System.TeamProject] = '{0}' and Source.[System.State] &lt;&gt; 'Removed' and Source.[System.WorkItemType] in ('Epic','Feature','User Story')) and ([System.Links.LinkType] = 'System.LinkTypes.Hierarchy-Forward') and (Target.[System.State] &lt;&gt; 'Removed' and Target.[System.WorkItemType] in ('Epic','Feature','User Story')) mode(Recursive)" />
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-8.0.0.0" newVersion="8.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>